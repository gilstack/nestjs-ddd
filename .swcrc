{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "loose": true,
    "externalHelpers": false,
    "minify": {
      "compress": {
        "unused": false
      },
      "mangle": false
    }
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true
}
